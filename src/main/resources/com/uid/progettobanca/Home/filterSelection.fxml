<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="980.0" styleClass="vbox-with-rounded-border" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.uid.progettobanca.controller.HomeController.FilterSelectionController">
   <children>
      <AnchorPane>
         <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="70.0">
               <children>
                  <VBox alignment="CENTER_LEFT" spacing="3.0">
                     <children>
                        <Label styleClass="titleLabel" text="Filtra" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <HBox>
         <children>
            <VBox prefWidth="475.0" spacing="5.0">
               <children>
                  <Label styleClass="subtitleLabel" text="Tag" />
                  <HBox prefHeight="100.0" prefWidth="200.0" styleClass="vbox-with-rounded-border">
                     <children>
                        <VBox>
                           <children>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="shopping" mnemonicParsing="false" />
                                    <Label text="Shopping" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="benessere" mnemonicParsing="false" />
                                    <Label text="Benessere" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="assicurazioneFinanza" mnemonicParsing="false" />
                                    <Label text="Assicurazione &amp; Finanza" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="intrattenimento" mnemonicParsing="false" />
                                    <Label text="Intrattenimento" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="multimediaElettronica" mnemonicParsing="false" />
                                    <Label text="Multimedia &amp; Elettronica" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="amiciFamiglia" mnemonicParsing="false" />
                                    <Label text="Amici &amp; Famiglia" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                           </children>
                        </VBox>
                        <VBox HBox.hgrow="ALWAYS">
                           <children>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="ciboSpesa" mnemonicParsing="false" />
                                    <Label text="Cibo &amp; Spesa" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="viaggi" mnemonicParsing="false" />
                                    <Label text="Viaggi" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="istruzione" mnemonicParsing="false" />
                                    <Label text="Istruzione" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="stipendio" mnemonicParsing="false" />
                                    <Label text="Stipendio" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="salute" mnemonicParsing="false" />
                                    <Label text="Salute" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <children>
                                    <CheckBox fx:id="altro" mnemonicParsing="false" />
                                    <Label text="Altro" />
                                 </children>
                                 <padding>
                                    <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                                 </padding>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox HBox.hgrow="ALWAYS" />
            <VBox prefWidth="275.0" spacing="5.0">
               <children>
                  <Label styleClass="subtitleLabel" text="Tipo di transazione" />
                  <VBox styleClass="vbox-with-rounded-border">
                     <children>
                        <HBox spacing="15.0" styleClass="border-bottom">
                           <children>
                              <RadioButton fx:id="inOut" mnemonicParsing="false" selected="true" />
                              <Label text="In entrata e in uscita" />
                           </children>
                           <padding>
                              <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                           </padding>
                        </HBox>
                        <HBox spacing="15.0" styleClass="border-bottom">
                           <children>
                              <RadioButton fx:id="inOnly" mnemonicParsing="false" />
                              <Label text="Solo in entrata" />
                           </children>
                           <padding>
                              <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                           </padding>
                        </HBox>
                        <HBox spacing="15.0">
                           <children>
                              <RadioButton fx:id="outOnly" mnemonicParsing="false" />
                              <Label text="Solo in uscita" />
                           </children>
                           <padding>
                              <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                           </padding>
                        </HBox>
                     </children>
                     <VBox.margin>
                        <Insets bottom="15.0" />
                     </VBox.margin>
                  </VBox>
                  <Label text="Label" />
                  <VBox styleClass="vbox-with-rounded-border">
                     <children>
                        <TextField fx:id="nameTextField" prefHeight="50.0" promptText="Nome Transazione" />
                     </children>
                  </VBox>
                  <HBox alignment="CENTER_RIGHT">
                     <children>
                        <Button fx:id="filterSelected" mnemonicParsing="false" onAction="#filterSelectedItems" prefHeight="50.0" prefWidth="150.0" styleClass="greenButton" text="Filtra selezionati" />
                     </children>
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  </HBox>
               </children></VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="50.0" left="75.0" right="75.0" top="30.0" />
   </padding>
