<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER_LEFT" onMouseClicked="#openQuickSend" prefHeight="60.0" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.uid.progettobanca.controller.OperationsController.ContactController">
   <children>
      <ImageView fx:id="users" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="nameLabel" text="Nome Cognome" />
            <Label fx:id="ibanLabel" text="IT12 1234 1234 1234 5678 6789" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="20.0" top="10.0" />
   </padding>
</HBox>
